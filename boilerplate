package main

import (
	"bufio"
	"os"
	"strconv"
)

func stringsToInts(strs []string) ([]int, error) {
	ints := make([]int, len(strs))
	for i := range strs {
		var err error
		ints[i], err = strconv.Atoi(strs[i])
		if err != nil {
			return nil, err
		}
	}
	return ints, nil
}

func solve() error {
	file, err := os.Open("dayX")
	if err != nil {
		return err
	}
	defer file.Close()

	// data, err := csv.NewReader(file).Read()
	// if err != nil {
	// 	return err
	// }
	// intList, err := stringsToInts(data)
	// if err != nil {
	// 	return err
	// }

	scanner := bufio.NewScanner(file)
	for scanner.Scan() {
		_ = scanner.Text()
	}

	return nil
}

func main() {
	if err := solve(); err != nil {
		panic(err)
	}
}
